{
  "src/day*.rs": {
    "type": "day",
    "template": [
      "#[aoc(day{}, part1)]",
      "fn solve_part1(input: &str) -> usize {",
      "    todo!()",
      "}",
      "",
      "#[aoc(day{}, part2)]",
      "fn solve_part2(input: &str) -> usize {",
      "    todo!()",
      "}",
      "",
      "#[cfg(test)]",
      "mod tests {",
      "    use super::*;",
      "",
      "    const SAMPLE_INPUT: &str = todo!();",
      "",
      "    #[test]",
      "    fn part1_example() {",
      "        assert_eq!(solve_part1(SAMPLE_INPUT), todo!());",
      "    }",
      "",
      "    #[test]",
      "    fn part1_mine() {",
      "        assert_eq!(solve_part1(&crate::get_input({})), todo!());",
      "    }",
      "",
      "    #[test]",
      "    fn part2_example() {",
      "        assert_eq!(solve_part2(SAMPLE_INPUT), todo!());",
      "    }",
      "",
      "    #[test]",
      "    fn part2_mine() {",
      "        assert_eq!(solve_part2(&crate::get_input({})), todo!());",
      "    }",
      "}"
    ]
  }
}
