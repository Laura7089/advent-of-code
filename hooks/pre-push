#!/usr/bin/bash

for i in 2019/*/; do
    (
        cd "$i" || exit 1
        printf "\n==================\n"
        printf "Testing %s...\n" "$i"
        python3.8 solver.py >/dev/null || exit 1
    )
done

printf "================================\n"
printf "All tests successful. Pushing...\n"
printf "================================\n"
