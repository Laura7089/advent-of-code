{
    "src/*.rs": {
        "type": "source"
    },
    "src/day*.rs" : {
        "type": "day",
        "template": [
            "#[aoc_generator(day{})]",
            "fn parse_input(input: &str) -> Vec<usize> {",
            "    input.split(\",\").map(|n| n.parse().unwrap()).collect()",
            "}",
            "",
            "#[aoc(day{}, part1)]",
            "fn solve_part1(input: &[usize]) -> usize {",
            "    unimplemented!()",
            "}",
            "",
            "#[aoc(day{}, part2)]",
            "fn solve_part2(input: &[usize]) -> usize {",
            "    unimplemented!()",
            "}",
            "",
            "#[cfg(test)]",
            "mod tests {",
            "    use super::*;",
            "",
            "    const EXAMPLE_INPUT: &'static str = unimplemented!()",
            "",
            "    #[test]",
            "    fn part1_example() {",
            "        assert_eq!(solve_part1(&parse_input(&EXAMPLE_INPUT)), unimplemented!());",
            "    }",
            "",
            "    #[test]",
            "    fn part2_example() {",
            "        assert_eq!(solve_part2(&parse_input(&EXAMPLE_INPUT)), unimplemented!());",
            "    }",
            "}"
        ]
    }
}
